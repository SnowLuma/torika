{
  "name": "@torika/renderer",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src/*": {
      "import": "./src/*.ts",
      "types": "./src/*.ts"
    },
    "./dist/*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@torika/default-theme-react": "workspace:*"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "marked": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
